{
  "name": "react-data-access",
  "version": "0.0.1",
  "description": "An example of how to do data access management with react and redux",
  "main": "js/main.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register --recursive",
    "watch": "mkdir -p build && watchify . -o build/build.js -dv",
    "build": "mkdir -p build && NODE_ENV=production browserify . > build/build.js",
    "serve": "open http://localhost:5526 && python -m SimpleHTTPServer 5526",
    "lint": "eslint ."
  },
  "author": "Taylor Baldwin <taylorbaldwin@gmail.com> (https://tbaldw.in)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.13.1",
    "expect": "^1.13.4",
    "mocha": "^2.3.4",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "isomorphic-fetch": "^2.2.0",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.6",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.2"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "react",
            "es2015"
          ],
          "plugins": [
            "transform-object-rest-spread"
          ]
        }
      ]
    ]
  }
}
