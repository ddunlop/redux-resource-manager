{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "import/prefer-default-export": "off",
    "no-console": "off",
    "no-param-reassign": "off",
    "no-use-before-define": "off",
    "object-shorthand": "off",
    "prefer-arrow-callback": "off",
  },
  "env": {
      "browser": true
  },
  "globals": {
    "fetch": false
  }
}
